<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".settings.ChangePrefix">

    <View
        android:id="@+id/my_rectangle4"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:background="#FF8000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_rectangle4" />

    <Button
        android:id="@+id/back_b"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="2dp"
        app:backgroundTint="#FF8000"
        android:background="@drawable/button"
        android:text="@string/back"
        app:layout_constraintBottom_toBottomOf="@+id/my_rectangle4"
        app:layout_constraintStart_toStartOf="@+id/my_rectangle4"
        app:layout_constraintTop_toTopOf="@+id/my_rectangle4"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/change_prefix"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/my_rectangle4"
        app:layout_constraintEnd_toEndOf="@+id/my_rectangle4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/my_rectangle4" />

</androidx.constraintlayout.widget.ConstraintLayout>